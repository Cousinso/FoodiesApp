<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.froupapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/burger"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/burger_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FroupApplication">
        <activity android:name=".WaitingForGroupActivity">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".FoodSelectionActivity" />
        </activity>
        <activity android:name=".GroupChatLogActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".GroupChatSelectionActivity" />
        </activity>
        <activity android:name=".NewGroupChatActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".GroupChatSelectionActivity" />
        </activity>
        <activity android:name=".GroupChatSelectionActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".LatestMessagesActivity" />
        </activity>
        <activity android:name=".ProfileTestActivity" />
        <activity android:name=".SelectedFoodActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FoodSelectionActivity" />
        </activity>
        <activity android:name=".FoodSelectionActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".ChatLogActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".LatestMessagesActivity" />
        </activity>
        <activity android:name=".NewMessageActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".LatestMessagesActivity" />
        </activity>
        <activity android:name=".LatestMessagesActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FoodSelectionActivity" />
        </activity>
        <activity android:name=".Register1Activity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".Register2Activity" />
        <activity android:name=".UserProfileActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FoodSelectionActivity" />
        </activity>
        <activity android:name=".Register3Activity" />
        <activity android:name=".Register4Activity"/>
        <activity android:name=".Register5Activity"/>
    </application>

</manifest>